<div id="container">
	<div class="row">
		<div class="col s6">
			<span id="text"> Welcome, {{username}}</span>
			<div class="selectDiv input-field">
				<select id="selectday">
					<option value="" disabled selected>Select Date Range</option>
					<option value="day">Day</option>
					<option value="week">Week</option>
					<option value="month">Month</option>
				</select>
			</div>
		</div>
	</div>
	<h4 id="textdata">Your Expenses</h4>
	<div class="row">
		<div class="col s12 m6 l6 min-height-300">
			<section id="piechart">
				<canvas id="pie"></canvas>
			</section>
		</div>
		<div class="col s12 m6 l6 min-height-300">
			<section id="linechart">
				<canvas id="bar"></canvas>
			</section>
		</div>
	</div>
	<div class="row">
		<div class="s12 m6 l6 min-height-300">
			<section id="doughnutchart">
				<canvas id="doughnut"></canvas>
			</section>
		</div>
	</div>

</div>